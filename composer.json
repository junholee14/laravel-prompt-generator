{
    "name": "vsolution-dev/laravel-prompt-generator",
    "description": "Laravel Prompt Generator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "vsolution-dev\\LaravelPromptGenerator\\": "src/"
        }
    },
  "autoload-dev": {
        "psr-4": {
            "vsolution-dev\\LaravelPromptGenerator\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "junholee14",
            "email": "junho2545@naver.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "scripts": {
      "inspect": "./vendor/bin/pint --test"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^6.23|^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.23|^2.6",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/phpstan": "^1.10"
    },
    "extra": {
      "laravel": {
        "providers": [
          "vsolution-dev\\LaravelPromptGenerator\\PromptGeneratorServiceProvider"
        ]
      }
    }
}
